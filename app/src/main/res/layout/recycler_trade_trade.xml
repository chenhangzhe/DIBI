<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">
    <ImageView
        android:id="@+id/iv_ttItemType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/sls_label_type_buy_sell"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/tv_ttItemPCoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:textSize="15sp"
        android:textStyle="bold"
        android:textColor="@color/bk2C"
        app:layout_constraintLeft_toRightOf="@id/iv_ttItemType"
        app:layout_constraintTop_toTopOf="@id/iv_ttItemType"
        app:layout_constraintBottom_toBottomOf="@id/iv_ttItemType"/>
    <TextView
        android:id="@+id/tv_ttItemTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemPCoin"/>

    <TextView
        android:id="@+id/tv_ttItemPriceName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/price"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toLeftOf="@id/iv_ttItemType"
        app:layout_constraintTop_toBottomOf="@id/iv_ttItemType"/>
    <TextView
        android:id="@+id/tv_ttItemPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_ttItemPriceName"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemPriceName"/>
    <TextView
        android:id="@+id/tv_ttItemPriceCoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_ttItemPrice"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemPriceName"/>
    <TextView
        android:id="@+id/tv_ttItemAmount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toRightOf="@id/tv_ttItemTime"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemPriceName"/>
    <TextView
        android:id="@+id/tv_ttItemAmountName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/amount"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toLeftOf="@id/tv_ttItemAmount"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemPriceName"/>

    <TextView
        android:id="@+id/tv_ttItemTotalName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/totalPrice"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toLeftOf="@id/iv_ttItemType"
        app:layout_constraintTop_toBottomOf="@id/tv_ttItemPriceName"/>
    <TextView
        android:id="@+id/tv_ttItemTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_ttItemTotalName"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemTotalName"/>
    <TextView
        android:id="@+id/tv_ttItemFee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toRightOf="@id/tv_ttItemTime"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemTotalName"/>
    <TextView
        android:id="@+id/tv_ttItemFeeName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/fee"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toLeftOf="@id/tv_ttItemFee"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_ttItemTotalName"/>
    <View
        android:layout_width="1dp"
        android:layout_height="12dp"
        app:layout_constraintTop_toBottomOf="@id/tv_ttItemTotalName"/>
</androidx.constraintlayout.widget.ConstraintLayout>