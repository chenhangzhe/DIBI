<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">
    <TextView
        android:id="@+id/tv_teItemRetreat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/sp1_solid_purple_77"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="8dp"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:text="@string/retreat"
        android:textSize="12sp"
        android:textColor="@color/white"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/iv_teItemType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/sls_label_type_buy_sell"
        android:layout_marginStart="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_teItemRetreat"
        app:layout_constraintBottom_toBottomOf="@id/tv_teItemRetreat"/>
    <TextView
        android:id="@+id/tv_teItemPCoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:textSize="15sp"
        android:textStyle="bold"
        android:textColor="@color/bk2C"
        app:layout_constraintLeft_toRightOf="@id/iv_teItemType"
        app:layout_constraintTop_toTopOf="@id/tv_teItemRetreat"
        app:layout_constraintBottom_toBottomOf="@id/tv_teItemRetreat"/>
    <TextView
        android:id="@+id/tv_teItemTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:textSize="15sp"
        android:textColor="@color/bk2C"
        app:layout_constraintLeft_toRightOf="@id/tv_teItemPCoin"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemPCoin"/>

    <TextView
        android:id="@+id/tv_teItemPriceName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/price"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toLeftOf="@id/iv_teItemType"
        app:layout_constraintTop_toBottomOf="@id/tv_teItemRetreat"/>
    <TextView
        android:id="@+id/tv_teItemPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_teItemPriceName"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemPriceName"/>
    <TextView
        android:id="@+id/tv_teItemPriceCoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_teItemPrice"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemPriceName"/>
    <TextView
        android:id="@+id/tv_teItemAmount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toRightOf="@id/tv_teItemRetreat"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemPriceName"/>
    <TextView
        android:id="@+id/tv_teItemAmountName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/amount"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toLeftOf="@id/tv_teItemAmount"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemPriceName"/>

    <TextView
        android:id="@+id/tv_teItemTotalName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/totalPrice"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toLeftOf="@id/iv_teItemType"
        app:layout_constraintTop_toBottomOf="@id/tv_teItemPriceName"/>
    <TextView
        android:id="@+id/tv_teItemTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_teItemTotalName"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemTotalName"/>
    <TextView
        android:id="@+id/tv_teItemTotalCoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintLeft_toRightOf="@id/tv_teItemTotal"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemTotalName"/>
    <TextView
        android:id="@+id/tv_teItemDeal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toRightOf="@id/tv_teItemRetreat"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemTotalName"/>
    <TextView
        android:id="@+id/tv_teItemDealVol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/dealVolume"
        android:textSize="12sp"
        android:textColor="@color/gy8A"
        app:layout_constraintRight_toLeftOf="@id/tv_teItemDeal"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_teItemTotalName"/>

    <LinearLayout
        android:id="@+id/ll_teItemTarget"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="4dp"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="@id/iv_teItemType"
        app:layout_constraintRight_toRightOf="@id/tv_teItemRetreat"
        app:layout_constraintTop_toBottomOf="@id/tv_teItemTotalName">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/touch"
            android:textSize="12sp"
            android:textColor="@color/gy8A"/>
        <TextView
            android:id="@+id/tv_teItemTarget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="--"
            android:textSize="12sp"
            android:textColor="@color/gy8A"/>
    </LinearLayout>
    <View
        android:layout_width="1dp"
        android:layout_height="12dp"
        app:layout_constraintTop_toBottomOf="@id/ll_teItemTarget"/>
</androidx.constraintlayout.widget.ConstraintLayout>